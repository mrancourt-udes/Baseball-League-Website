<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>Routes</servlet-name>
        <servlet-class>ligueBaseballServlet.Routes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FormHandler</servlet-name>
        <servlet-class>ligueBaseballServlet.FormHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Equipes</servlet-name>
        <servlet-class>ligueBaseballServlet.Equipes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Joueurs</servlet-name>
        <servlet-class>ligueBaseballServlet.Joueurs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Arbitres</servlet-name>
        <servlet-class>ligueBaseballServlet.Arbitres</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Matchs</servlet-name>
        <servlet-class>ligueBaseballServlet.Matchs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>ligueBaseballServlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>ligueBaseballServlet.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Suppression</servlet-name>
        <servlet-class>ligueBaseballServlet.Suppression</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>ligueBaseball.Upload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Routes</servlet-name>
        <url-pattern>/Routes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FormHandler</servlet-name>
        <url-pattern>/FormHandler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Equipes</servlet-name>
        <url-pattern>/Equipes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Joueurs</servlet-name>
        <url-pattern>/Joueurs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Arbitres</servlet-name>
        <url-pattern>/Arbitres</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Matchs</servlet-name>
        <url-pattern>/Matchs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Upload</servlet-name>
        <url-pattern>/Upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Suppression</servlet-name>
        <url-pattern>/Suppression</url-pattern>
    </servlet-mapping>
    <error-page>
        <error-code>404</error-code>
        <location>/404.html</location>
    </error-page>
</web-app>
